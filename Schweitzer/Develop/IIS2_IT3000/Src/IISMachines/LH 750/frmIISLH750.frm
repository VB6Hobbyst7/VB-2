End Sub

Private Sub cmdAuto_Click()
    If cmdAuto.Tag = "0" Then
        timAuto.Enabled = True
        cmdAuto.Caption = "중지(&S)"
        cmdAuto.Tag = "1"
        cmdSend.Enabled = False
        txtBarNo.Enabled = False
    Else
        timAuto.Enabled = False
        cmdAuto.Caption = "자동전송(&A)"
        cmdAuto.Tag = "0"
        cmdSend.Enabled = True
        txtBarNo.Enabled = True
        txtBarNo.SetFocus
    End If
End Sub

Private Sub cmdSend_Click()
    Dim vOrdChk     As Variant  'Spread의 오더전송 여부
    Dim vBarNo      As Variant  'Spread의 바코드번호
    Dim i           As Long

    If tblReady.DataRowCnt < 1 Then Exit Sub

    With tblReady
        For i = 1 To .DataRowCnt
            Call .GetText(1, i, vOrdChk)
            If CStr(vOrdChk) = "" Then
                Call .GetText(2, i, vBarNo)
                
                mIntLib.State = "O"
                mIntLib.SndPhase = 1
                mOrder.ClsClear
                mOrder.BarNo = CStr(vBarNo)
                Call SendOrder
                Exit Sub
            End If
        Next i
    End With
End Sub

Private Sub cmdAlarm_Click()
    Dim objErrorShow As clsIISErrorShow     '에러폼 표시 클래스

    Set objErrorShow = New clsIISErrorShow
    Call objErrorShow.ShowErrors(mIntErrors)
