'-----------------------------------------------------------------------------'
'   ±â´É : Áö¼öÇü½ÄÀÇ °á°ú¸¦ Á¤¼öÇü½ÄÀÇ °á°ú·Î º¯È¯
'   ÀÎ¼ö :
'       - pResult : Áö¼öÇü½ÄÀÇ °á°ú
'   ¹ÝÈ¯ : Á¤¼öÇü½ÄÀÇ °á°ú
'-----------------------------------------------------------------------------'
Private Function GetNumber(ByVal pResult As String) As String
    Dim strFlag     As String   '+, -
    Dim sngResult   As Single   'Á¤¼öºÎ °á°ú
    Dim lngFlag     As Long     'Áö¼öºÎ °á°ú
    Dim Pos         As Long

    Pos = InStr(pResult, "E")
    sngResult = CSng(Mid$(pResult, 1, Pos - 1))
    lngFlag = CLng(Mid$(pResult, Pos + 2))
    strFlag = Mid$(pResult, Pos + 1, 1)
    
    If strFlag = "+" Then
        sngResult = sngResult * (CLng("1" & String$(lngFlag, "0")))
    ElseIf strFlag = "-" Then
        sngResult = sngResult / (CLng("1" & String$(lngFlag, "0")))
    End If
    
    GetNumber = CStr(sngResult)
End Function

