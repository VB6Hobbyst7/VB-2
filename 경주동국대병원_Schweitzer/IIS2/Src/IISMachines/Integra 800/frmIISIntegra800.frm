                End If
                Exit For
            End If
        Next i
    End With
End Sub

'-----------------------------------------------------------------------------'
'   ±â´É : ÇØ´ç ¹ÙÄÚµå¹øÈ£ÀÇ ¸ðµç Ç×¸ñÀÌ °á°ú¼ö½ÅÀ» Çß´ÂÁö È®ÀÎ
'          ¸ðµçÇ×¸ñÀ» ¼ö½Å½Ã¿¡´Â tblReady¿¡ »èÁ¦
'   ¹ÝÈ¯
'       - True(¸ðµÎ¼ö½Å), False(ÀÏºÎ¼ö½Å)
'-----------------------------------------------------------------------------'
Private Function IsAllReceive(ByVal pBarNo As String) As Boolean
    Dim vBarNo      As Variant  'SpreadÀÇ ¹ÙÄÚµå¹øÈ£
    Dim lngSndCnt   As Long     'Àü¼ÛÇÑ °³¼ö
    Dim lngRcvCnt   As Long     '¼ö½ÅÇÑ °³¼ö
    Dim i           As Long
    
    With tblReady
        For i = 1 To .DataRowCnt
            Call .GetText(TReadyEnum.ccBarNo, i, vBarNo)
            
            If CStr(vBarNo) = pBarNo Then
                .Row = i
                .Col = TReadyEnum.ccSndCnt: lngSndCnt = CLng(.Text)
                .Col = TReadyEnum.ccRcvCnt: lngRcvCnt = CLng(.Text)
                
                lngRcvCnt = lngRcvCnt + 1
                If lngRcvCnt = lngSndCnt Then
                    Call .DeleteRows(i, 1)
                    IsAllReceive = True
                Else
                    .Col = TReadyEnum.ccOrdChk: .Text = "¼ö½ÅÁß"
                    .Col = TReadyEnum.ccRcvCnt: .Text = CStr(lngRcvCnt)
                    IsAllReceive = False
                End If
                
                If .DataRowCnt = 0 Then tmrResult.Enabled = False
'                Debug.Print tmrResult.Enabled
                Exit Function
            End If
        Next i
    End With
    
    '## °Ë»ç´ë»ó ¸®½ºÆ®¿¡ ¾ø´Â°æ¿ì ÄÃ·º¼Ç¿¡¼­ »èÁ¦ÇÏµµ·Ï ÇÏ±â À§ÇØ
    IsAllReceive = True
End Function

