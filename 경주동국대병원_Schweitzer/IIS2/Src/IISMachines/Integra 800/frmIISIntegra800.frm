Private Sub SaveServer(ByVal pIntInfo As clsIISIntInfo)
    Dim objAccInfo  As clsIISAccInfo    'Á¢¼ö³»¿ª Å¬·¡½º
    Dim vBarNo      As Variant 'SpreadÀÇ ¹ÙÄÚµå¹øÈ£
    Dim strBarNo    As String  '¹ÙÄÚµå¹øÈ£
