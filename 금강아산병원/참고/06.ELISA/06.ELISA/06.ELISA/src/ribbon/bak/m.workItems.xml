            <RecentItems CommandName="ActionRecentTest" EnablePinning="false"/>
          </ApplicationMenu.RecentItems>
          <MenuGroup>
            <Button CommandName="ActionNewTest"/>
            <Button CommandName="ActionOpen"/>
            <Button CommandName="ActionSave"/>
            <Button CommandName="ActionSaveAs"/>
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="ActionOption"/>
