          <MenuGroup>
            <Button CommandName="ActionAbout"/>
            <Button CommandName="ActionSDB"/>
          </MenuGroup>
          <MenuGroup>
            <Button CommandName="ActionExit"/>
          </MenuGroup>
        </ApplicationMenu>
      </Ribbon.ApplicationMenu>
      <Ribbon.QuickAccessToolbar>
        <QuickAccessToolbar>
          <QuickAccessToolbar.ApplicationDefaults>
            <Button CommandName="ActionNewTest" ApplicationDefaults.IsChecked="true"/>
            <Button CommandName="ActionOpen" ApplicationDefaults.IsChecked="false"/>
            <Button CommandName="ActionSave" ApplicationDefaults.IsChecked="true"/>
            <Button CommandName="ActionSaveAs" ApplicationDefaults.IsChecked="false"/>
            <Button CommandName="ActionOption" ApplicationDefaults.IsChecked="true"/>
          </QuickAccessToolbar.ApplicationDefaults>
        </QuickAccessToolbar>
      </Ribbon.QuickAccessToolbar>
      <Ribbon.Tabs>
        <Tab CommandName="ActionTabTest">
          <Tab.ScalingPolicy>
            <ScalingPolicy>
              <ScalingPolicy.IdealSizes>
                <Scale Group="ActionGroupData" Size="Large"/>
                <Scale Group="ActionGroupDataFmt" Size="Large"/>
                <Scale Group="ActionGroupAnalyze" Size="Large"/>
              </ScalingPolicy.IdealSizes>
              <Scale Group="ActionGroupData" Size="Popup"/>
              <Scale Group="ActionGroupDataFmt" Size="Popup"/>
              <Scale Group="ActionGroupAnalyze" Size="Popup"/>
            </ScalingPolicy>
          </Tab.ScalingPolicy>
          <Group CommandName="ActionGroupData">
            <SizeDefinition>
              <ControlNameMap>
                <ControlNameDefinition Name="ActionPaste"/>
                <ControlNameDefinition Name="ActionNewTest"/>
                <ControlNameDefinition Name="ActionTestInfo"/>
              </ControlNameMap>
              <GroupSizeDefinition Size="Large">
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionPaste"/>
                <ColumnBreak/>
                <ControlSizeDefinition ControlName="ActionNewTest"/>
                <ControlSizeDefinition ControlName="ActionTestInfo"/>
              </GroupSizeDefinition>
            </SizeDefinition>
            <Button CommandName="ActionPaste"/>
            <Button CommandName="ActionNewTest"/>
            <ToggleButton CommandName="ActionTestProperty"/>
          </Group>
          <Group CommandName="ActionGroupDataFmt">
            <SizeDefinition>
              <ControlNameMap>
                <ControlNameDefinition Name="ActionFmtDefault"/>
                <ControlNameDefinition Name="ActionFmtManual"/>
                <ControlNameDefinition Name="ActionFmtLoad"/>
                <ControlNameDefinition Name="ActionFmtSave"/>
                <ControlNameDefinition Name="ActionFmtViewNames"/>
              </ControlNameMap>
              <GroupSizeDefinition Size="Large">
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionFmtDefault"/>
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionFmtManual"/>
                <ControlSizeDefinition ControlName="ActionFmtLoad"/>
                <ControlSizeDefinition ControlName="ActionFmtSave"/>
                <ControlSizeDefinition ControlName="ActionFmtViewNames"/>
              </GroupSizeDefinition>
            </SizeDefinition>
            <DropDownButton CommandName="ActionFmtDefault">
              <Button CommandName="ActionFmt3Item"/>
              <Button CommandName="ActionFmt2Item"/>
            </DropDownButton>
            <ToggleButton CommandName="ActionFmtManual"/>
            <Button CommandName="ActionFmtLoad"/>
            <Button CommandName="ActionFmtSave"/>
            <Button CommandName="ActionFmtViewNames"/>
          </Group>
          <Group CommandName="ActionGroupAnalyze">
            <SizeDefinition>
              <ControlNameMap>
                <ControlNameDefinition Name="ActionCalc"/>
                <ControlNameDefinition Name="ActionPrint"/>
                <ControlNameDefinition Name="ActionExportResult"/>
                <ControlNameDefinition Name="ActionExportData"/>
              </ControlNameMap>
              <GroupSizeDefinition Size="Large">
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionCalc"/>
                <ColumnBreak/>
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionPrint"/>
                <ColumnBreak/>
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionExportResult"/>
                <ControlSizeDefinition ImageSize="Large" ControlName="ActionExportData"/>
              </GroupSizeDefinition>
            </SizeDefinition>
            <Button CommandName="ActionCalc"/>
            <Button CommandName="ActionPrint"/>
            <Button CommandName="ActionExportResult"/>
            <Button CommandName="ActionExportData"/>
          </Group>
        </Tab>
      </Ribbon.Tabs>
