Private Sub EditRcvData()
    Dim objIntInfo  As clsIISIntInfo    'ÀÎÅÍÆäÀÌ½º °ËÃ¼Á¤º¸ Å¬·¡½º
    Dim objIntNms   As clsIISIntNms     'Àåºñº° °Ë»çÇ×¸ñ ÄÃ·º¼Ç Å¬·¡½º
    
    Dim strRcvBuf   As String   '¼ö½ÅÇÑ Data
    Dim strCode     As String   '¼ö½ÅÇÑ Classification Code
    Dim strBarNo    As String   '¼ö½ÅÇÑ ¹ÙÄÚµå¹øÈ£
    Dim strSeqNo    As String
    Dim strRackNo   As String   '¼ö½ÅÇÑ RackNo
    Dim strCupPos   As String   '¼ö½ÅÇÑ Cup Position
    Dim strIntBase  As String   '¼ö½ÅÇÑ Àåºñ±âÁØ °Ë»ç¸í
    Dim strResult   As String   '¼ö½ÅÇÑ °á°ú
    Dim strFlags    As String   '¼ö½ÅÇÑ °á°ú Flags
    Dim strInfo     As String   'Ãß°¡Á¤º¸
    Dim strOutput   As String
    Dim i           As Long
    
    strRcvBuf = mIntLib.Buffers(1).Buffers
    strCode = Trim$(Mid$(strRcvBuf, 1, 2))
    
    Select Case strCode
        Case "RB"   '## Start Inquiry Text
            'MSComm.Output = ACK
            'Call mIntLib.WriteLog(ACK, ccPCLog)
            
        Case "R"    '## TRI Inquiry Text
            'MSComm.Output = ACK
            Call mIntLib.WriteLog(ACK, ccPCLog)
            
            '## Rack No, Cup Position, ¹ÙÄÚµå¹øÈ£ Á¶È¸
            strBarNo = Trim$(Mid$(strRcvBuf, 14, 26))
            strRackNo = Mid$(strRcvBuf, 3, 4)
            strCupPos = Mid$(strRcvBuf, 7, 2)
            strSeqNo = Mid$(strRcvBuf, 9, 5)
            With mOrder
                .ClsClear
                .RackNo = strRackNo
                .CupPos = strCupPos
                .BarNo = strBarNo
                .SampleNo = strSeqNo
            End With
            Call GetOrder(strBarNo)
            
        Case "RE"   '## End Inquirty Text
            'MSComm.Output = ACK
            'Call mIntLib.WriteLog(ACK, ccPCLog)
            
        Case "DB"   '## Start Test Result Text
            'MSComm.Output = ACK
            'Call mIntLib.WriteLog(ACK, ccPCLog)
            
        Case "D"    '## Test Result Text
            '## Rack No, Cup Position, ¹ÙÄÚµå¹øÈ£ Á¶È¸
            'D 000501 0001            02001035    E001  2.60Pr002  75.1Pr003    98r 004   7.1r 005   3.4Nr007    80Pr008    75Pr009    61r 011  0.68r 097   132Nr098   4.3r 099   101r
            'D 000302 0016                1710300006    E001   184r 006  27.4r 007  19.3r 011    98r 013  1.03r 015  14.4r 018   113r 019  63.5r 027    98r DE[P C] 

            strRackNo = Mid$(strRcvBuf, 3, 4)
            strCupPos = Mid$(strRcvBuf, 7, 2)
            strSeqNo = Mid$(strRcvBuf, 10, 4)
            strBarNo = Trim(Mid$(strRcvBuf, 14, 26))
            
            Set objIntInfo = New clsIISIntInfo
            With objIntInfo
                .BarNo = strBarNo
                .SpcPos = strCupPos & "/" & strRackNo
            End With
            
            '## °á°úÀúÀå
            Set objIntNms = mIntLib.IntNms
            For i = 45 To Len(strRcvBuf) - 1 Step 11
                strIntBase = Mid$(strRcvBuf, i, 3)
                strResult = Trim$(Mid$(strRcvBuf, i + 3, 6))
                strFlags = Trim$(Mid$(strRcvBuf, i + 9, 1))
                strInfo = GetContent(strFlags)
                
                If objIntNms.ExistIntBase(strIntBase) Then
                    Call objIntInfo.IntResults.Add(strIntBase, objIntNms.GetIntNm(strIntBase), _
                         strResult, strResult, strInfo)
                End If
            Next i
            Call SaveServer(objIntInfo)
            Set objIntNms = Nothing
            Set objIntInfo = Nothing
            
            'MSComm.Output = ACK
            'Call mIntLib.WriteLog(ACK, ccPCLog)
            
        Case "DE"   '## End Test Result Text
           ' MSComm.Output = ACK
            'Call mIntLib.WriteLog(ACK, ccPCLog)
    End Select
End Sub

