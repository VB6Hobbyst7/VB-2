                    End With
            
                    Call SetPatInfo(strBarno, gHOSP.RSTTYPE)
                                
                    If gRow <= 0 Then
                        Exit Sub
                    End If
                        
                        
                    strTmp = Mid$(strRcvBuf, 51)
    
                    Do While Len(strTmp) >= 10
                        strIntBase = Mid$(strTmp, 2, 2)
                        strResult = Trim(Mid$(strTmp, 4, 6))
