Private Sub Phase_Serial_RAPIDLAB348()
    Dim Buffer      As Variant
    Dim BufChar     As String
    Dim lngBufLen   As Long
    Dim i           As Long

    lngBufLen = Len(pBuffer)
    
    For i = 1 To lngBufLen
        BufChar = Mid$(pBuffer, i, 1)
        Select Case intPhase
            Case 1      '## STX 얾쐑
                Select Case BufChar
                    Case STX
                        intPhase = 2
                        intBufCnt = 1
                        Erase strRecvData
                        ReDim Preserve strRecvData(intBufCnt)
                
                End Select
            Case 2      '## ETX 얾쐑
                Select Case BufChar
                    Case ETX
                        intPhase = 3
                    Case Else
                        strRecvData(intBufCnt) = strRecvData(intBufCnt) & BufChar
                End Select
            Case 3      '## EOT 얾쐑
                Select Case BufChar
                    Case EOT
                        Call SerialRcvData_RAPIDLAB348
                        intPhase = 1
                End Select
        End Select
    Next i
    
End Sub


Private Sub Phase_Serial_BS240()
    Dim Buffer      As Variant
    Dim BufChar     As String
    Dim lngBufLen   As Long
    Dim i           As Long

    lngBufLen = Len(pBuffer)
    
    For i = 1 To lngBufLen
        BufChar = Mid$(pBuffer, i, 1)
        Select Case BufChar
            Case ENQ
                comEqp.Output = ACK
                SetRawData "[Tx]" & ACK
                
            Case EOT
                If strState = "Q" Then
                    comEqp.Output = ENQ
                    SetRawData "[Tx]" & ENQ
                End If
                'comEqp.Output = ACK
                'SetRawData "[Tx]" & ACK
                
            Case ACK
                If strState = "Q" Then
                    Call SendOrder_BS240
                End If
            Case STX
                RcvBuffer = ""
        
            Case ETX
                Call SerialRcvData_BS240
                RcvBuffer = ""
                
                comEqp.Output = ACK
                SetRawData "[Tx]" & ACK
                
            Case Else
                RcvBuffer = RcvBuffer & BufChar
        End Select
    Next i
    
End Sub



