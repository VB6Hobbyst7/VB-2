                                .spdResult.MaxRows = lsRstRow
                            End If
    
                            '훺泰 칩쟕, 써쎀 콱 칩쟕
                            strMachResult = strResult
                            If strQCTemp = "1" Then
                                strResult = SetResult(strResult, strIntBase)
                            End If
