            End If
            
            '## TIBC 첇절 UIBC,FE 챲矗 촺얯.
            If lngIntBase = 98 Then
                Mid$(strItems, 22, 1) = "1"     'FE
                Mid$(strItems, 23, 1) = "1"     'UIBC
