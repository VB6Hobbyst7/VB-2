Private Sub SendOrder_HITACHI7080()
    Dim strOutput   As String     '쩉쫜 온첇콼
    
    strOutput = ";" & mOrder.Function
    strOutput = strOutput & "48"
    strOutput = strOutput & Mid(mOrder.Order, 1, 48)
    strOutput = strOutput & "000000"
    
    'COMMENT윗찌 BARCODE 킨쫚
