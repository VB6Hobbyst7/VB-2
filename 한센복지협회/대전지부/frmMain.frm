Private Sub Phase_Serial_ADVIA2120()
    Dim Buffer      As Variant
    Dim BufChar     As String
    Dim lngBufLen   As Long
    Dim i           As Long

    lngBufLen = Len(pBuffer)
            
    For i = 1 To lngBufLen
        BufChar = Mid$(pBuffer, i, 1)

        Select Case intPhase
            Case 1                  ' 퉘邱 턅첉(MT 얾쐑)
                Select Case BufChar
                    Case Chr(2)          'STX
                        RcvBuffer = ""
                        
                    Case msMT                           ' 0(&H30), Initialize Message 쵕턗첇좒
                        RcvBuffer = ""
                        Call TransferToken              'Transfer_Token 쫚옷 턗
                        intPhase = 2

                    Case Else
                        'MT첇㉯ 썭(쵎, NACK)
                        Call InitialComm
                        intPhase = 1
                        Exit Sub
                End Select

            Case 2                                      ' Token Tranfer찌 얾 MT 얾쐑
                Select Case BufChar
                    Case Chr(21)         'NAK
                        Call InitialComm
                        intPhase = 1
                        
                    Case msMT
                        intPhase = 3

                    Case Chr(Asc(msMT) - 1)
                        msMT = Chr(Asc(msMT) - 1)
                        If Asc(msMT) < 0 Then
                            msMT = Chr(&H30)
                        End If
                        Call TransferToken              'Transfer_Token 쫚옷 턗
                        intPhase = 2

                    Case Else
                        Call TransferToken
                        intPhase = 2
                End Select

            Case 3                                      ' CheckSum STX첉 썭是 챵 좭쵔쟍 챦, Phase 3 쎀 4 쥃쟕
                Select Case Asc(BufChar)
                    Case 2          'STX
                        RcvBuffer = ""
                        intPhase = 4

                    Case Else
                        intPhase = 3

                End Select

            Case 4                                      'DataEdit(천쥠췕찌쨠 줮앁얙 S, Q, R Ъ쨰殆 얾) 얾쐑,
                Select Case Asc(BufChar)
                    Case 3            ' ETX
                        msMT = Left(RcvBuffer, 1)
                        Call Sleep(25)  'Delay Time 0.025 sec

                        comEqp.Output = msMT
                        SetRawData "[Tx]" & msMT
                        Call SerialRcvData_ADVIA2120
                        intPhase = 3

                    Case Else
                        RcvBuffer = RcvBuffer & BufChar

                End Select
        End Select
    Next i
    
    
End Sub


