            Call SetText(frmMain.spdOrder, "챲邨岵", intRow, colSTATE)
        Else
            mOrder.NoOrder = False
            mOrder.Order = strItems
        
            '-- 쵖講紬(Order) 킨쫚
            Call SetText(frmMain.spdOrder, "챲塚쩉", intRow, colSTATE)
        End If


        '-- 汰 Row
        gRow = intRow
        
    End With
    
End Sub

