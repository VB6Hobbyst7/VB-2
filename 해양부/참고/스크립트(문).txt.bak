CREATE TABLE RTDB.DT 
(
	DT_TS_ID                 NUMBER NOT NULL,
	DT_TIME                  DATE NOT NULL,
	DT_WSPEED                NUMBER (10,6),
	DT_WMSPEED               NUMBER (10,6),
	DT_WDIR                  NUMBER (10,6),
	DT_ATEMP                 NUMBER (10,6),
	DT_APRESS                NUMBER (10,6),
	DT_TIDE                  NUMBER (16,6),
	DT_FTIDE                 NUMBER (16,6),
	DT_PTIDE                 NUMBER (16,6),
	DT_SAL                   NUMBER (10,6),
	DT_WTEMP                 NUMBER (20,10),
	DT_VISIBILITY            NUMBER (10,6),
	REFERENCE                VARCHAR2 (10),
	DT_TIDE3                 NUMBER (16,6),
	DT_CONDUCTIVITY          NUMBER (10,6),
	DT_REF_TIME              DATE,
	DT_FLAG                  VARCHAR2 (1) DEFAULT 'D',
	REG_DATE                 DATE,
	TRACK_SEQ                VARCHAR2 (50),
	DT_SIGNIFI_WAVE          NUMBER,
	DT_MAX_WAVE              NUMBER,
	DT_SIGNIFI_WAVE_PERIOD   NUMBER,
	DT_MAX_WAVE_PERIOD       NUMBER,
	DT_INSOLATION            NUMBER,
	DT_LTIDE                 NUMBER (16,6),
	DT_LTIDE2                NUMBER (16,6),
	CONSTRAINT FK01_DT FOREIGN KEY (DT_TS_ID) REFERENCES RTDB.TIDAL_STATION (TS_ID) ON DELETE SET NULL
	DISABLE NOVALIDATE
)

CREATE TABLE RTDB.DT_MAX_DT_TIME //조위관측소 VPN 54개 정도
(
	DT_TS_ID   NUMBER NOT NULL,
	DT_TIME    DATE NOT NULL
)

CREATE TABLE RTDB.LOG_DT 
(
	DT_TS_ID      NUMBER NOT NULL,
	DT_TIME       DATE NOT NULL,
	LOG_ID        CHAR (4) NOT NULL,
	REG_DATE      DATE NOT NULL,
	LOG_CONTENT   VARCHAR2 (500) NOT NULL,
	OBS_POST_ID   VARCHAR2 (11) DEFAULT '000000' NOT NULL
)

CREATE TABLE RTDB.TIDAL_STATION 
(
	TS_ID             NUMBER NOT NULL,
	POST_CODE         VARCHAR2 (8),
	TS_NAME           VARCHAR2 (20),
	LOC_LATITUDE      NUMBER (10,6),
	LOC_LONGITUDE     NUMBER (10,6),
	DEFAULT_TIDE      VARCHAR2 (30),
	OBS_POST_ID       VARCHAR2 (11) NOT NULL,
	COLLECTION_TYPE   CHAR (1) DEFAULT 'V' NOT NULL,
	VPN_IP            VARCHAR2 (17),
	KORDI_MODELING    CHAR (1) DEFAULT 'N' NOT NULL,
	FTP_USER          VARCHAR2 (50),
	FTP_PW            VARCHAR2 (50),
	FTP_PORT          NUMBER,
	FTP_REMOTEDIR     VARCHAR2 (200),
	FTP_LOCALDIR      VARCHAR2 (200),
	FTP_PASSIVE       NUMBER,
	COLLECTION_FILE   VARCHAR2 (10)
)


