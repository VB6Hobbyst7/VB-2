    Dim sBarcode    As String
    
    GetSampleInfoW = -1
    
    sBarcode = Trim(GetText(frmInterface.vasID, asRow, colBARCODE))
    
    If sBarcode = "" Then
        Exit Function
    End If
    
          SQL = " SELECT DISTINCT REQ_DT AS 접수일자"
    SQL = SQL & ", LOT_NO AS 차트번호"
    SQL = SQL & ", REQ_SEQ AS 내원번호"
    SQL = SQL & ", '입원' AS 입외"
    SQL = SQL & ", '홍길동' AS 이름"
    SQL = SQL & ", '남자' AS 성별"
    SQL = SQL & ", REQ_SEQ AS 나이" & vbCrLf
